function [Xnew ,dx ,n]= gradientDescent(objectivefunction ,StartP)
syms x y;
maxiter = 1000;
XSteps=zeros(maxiter , 1);
Error = 1e-6;
alpha = 0.1 ; 
dx = inf; 
n=0; 
Xnew =  ;
grad =  gradient(objectivefunction , [x,y]);

while or(dx>=Error, n <= maxiter)  
    
    f=subs(grad,{x,y},StartP);
    Xnew =  - alpha*f ;
    
    if ~isfinite(Xnew)
    error('x is inf or NaN')
    end
    
    X = Xnew ;
    n= n+1 ;
    dx= norm(xnew-x);
end

 
end
 
 
